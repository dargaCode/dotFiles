export PS1="\n\[\033[00;36m\]\T \w \[\033[00m\] \[\033[00;31m\] \n\u $\[\033[00m\]"

alias showinvis='defaults write com.apple.finder AppleShowAllFiles YES; killall Finder /System/Library/CoreServices/Finder.app; echo Displaying invisible files'
alias hideinvis='defaults write com.apple.finder AppleShowAllFiles  NO; killall Finder /System/Library/CoreServices/Finder.app; echo Hiding invisible files'
